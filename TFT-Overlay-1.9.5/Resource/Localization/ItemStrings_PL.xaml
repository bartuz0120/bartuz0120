    <s:String x:Key="StatikkShiv" xml:space="preserve">Kosa Statikka</s:String>
    <s:String x:Key="StatikkShiv1" xml:space="preserve">&#10;Co każdy 3. atak zadaje 100 magicznych obrażeń.</s:String>
    <!--Sword Breaker-->
    <s:String x:Key="SwordBreaker" xml:space="preserve">Łamimiecz</s:String>
    <s:String x:Key="SwordBreaker1" xml:space="preserve">&#10;Atak ma szansę na rozbrojenie.</s:String>
    <!--Sword of the Divine-->
    <s:String x:Key="SwordDivine" xml:space="preserve">Boski Miecz</s:String>
