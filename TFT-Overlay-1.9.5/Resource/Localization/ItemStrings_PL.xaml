    <!--Locket of the Iron Solari-->
    <s:String x:Key="LocketSolari" xml:space="preserve">Naszyjnik Żelaznych Solari</s:String>
    <s:String x:Key="LocketSolari1" xml:space="preserve">&#10;Na początku walki, wszyscy sąsiadujący sojusznicy otrzymują tarcze o wartości 200.</s:String>
    <!--Luden's Echo-->
    <s:String x:Key="LudensEcho" xml:space="preserve">Echo Luden</s:String>
    <s:String x:Key="LudensEcho1" xml:space="preserve">&#10;Umiejętność zadaje 200 obrażeń rozpryskowych.</s:String>
