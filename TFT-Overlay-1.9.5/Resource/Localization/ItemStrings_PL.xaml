    <!--Darkin-->
    <s:String x:Key="Darkin" xml:space="preserve">Darkin</s:String>
    <s:String x:Key="Darkin1" xml:space="preserve">&#10;Posiadacz jest także Demon.</s:String>
    <!--Force of Nature-->
    <s:String x:Key="ForceOfNature" xml:space="preserve">Siła Natury</s:String>
    <s:String x:Key="ForceOfNature1" xml:space="preserve">&#10;Dostajesz +1 Miejsce w drużynie.</s:String>
    <!--Frozen Mallet-->
    <s:String x:Key="FrozenMallet" xml:space="preserve">Lodowy Młot</s:String>
    <s:String x:Key="FrozenMallet1" xml:space="preserve">&#10;Posiadacz jest także Mroźnym.</s:String>
    <!--Knight's Vow-->
    <s:String x:Key="KnightsVow" xml:space="preserve">Przysięga Rycerska</s:String>
    <s:String x:Key="KnightsVow1" xml:space="preserve">&#10;Posiadacz jest także Rycerzem.</s:String>
    <!--Runaan's Hurricane-->
    <s:String x:Key="RunaansHurricane" xml:space="preserve">Huragan Runaana</s:String>
