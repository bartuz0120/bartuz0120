    <!--Dragon's Claw-->
    <s:String x:Key="DragonsClaw" xml:space="preserve">Smoczy Pazur</s:String>
    <s:String x:Key="DragonClaw1" xml:space="preserve">&#10;Daje 83% odporności na magiczne obrażenia.</s:String>
    <!--Frozen Heart-->
    <s:String x:Key="FrozenHeart" xml:space="preserve">Mroźne Serce</s:String>
    <s:String x:Key="FrozenHeart1" xml:space="preserve">&#10;Sąsiadujący przeciwnicy tracą 20% prędkości ataku.</s:String>
    <!--Guardian Angel-->
    <s:String x:Key="GuardianAngel" xml:space="preserve">Anioł Stróż</s:String>
    <s:String x:Key="GuardianAngel1" xml:space="preserve">&#10;Posiadacz odradza się z 500 punktami zdrowia.</s:String>
    <!--Guinsoo's Rageblade-->
    <s:String x:Key="GuinsoosRageblade" xml:space="preserve">Ostrze Gniewu Guinsoo</s:String>
    <s:String x:Key="GuinsoosRageblade1" xml:space="preserve">&#10;Atak zapewnia dodatkowe 3% prędkości ataku (Kumuluje się w nieskończoność).</s:String>
    <!--Hextech Gunblade-->
    <s:String x:Key="HextechGunblade" xml:space="preserve">Miecz-Pistolet Hextech</s:String>
    <s:String x:Key="HextechGunblade1" xml:space="preserve">&#10;Leczy za 25% wszystkich zadanych obrażeń.</s:String>
    <!--Hush-->
    <s:String x:Key="Hush" xml:space="preserve">Uciszacz</s:String>
