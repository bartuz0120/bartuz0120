    <!--Thornmail-->
    <s:String x:Key="Thornmail" xml:space="preserve">Kolczasta Kolczuga</s:String>
    <s:String x:Key="Thornmail1" xml:space="preserve">&#10;Odbija 35% odtrzymanych fizycznych obrażeń.</s:String>
    <!--Titanic Hydra-->
    <s:String x:Key="TitanicHydra" xml:space="preserve">Kolosalna Hydra</s:String>
    <s:String x:Key="TitanicHydra1" xml:space="preserve">&#10;Ataki zadają 10% maksymalnego zdrowia posiadacza jako obrażenia rozpryskowe.</s:String>
    <!--Warmog's Armor-->
    <s:String x:Key="WarmogsArmor" xml:space="preserve">Plemienna Zbroja</s:String>
    <s:String x:Key="WarmogsArmor1" xml:space="preserve">&#10;Posiadacz odnawia 3% maksymalnego zdrowia co sekundę.</s:String>
