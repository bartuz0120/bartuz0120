    <s:String x:Key="Bloodthirster" xml:space="preserve">Krwiopijec</s:String>
    <s:String x:Key="Bloodthirster1" xml:space="preserve">&#10;Atak leczy za 50% zadanych obrażeń.</s:String>
    <!--Cursed Blade-->
    <s:String x:Key="CursedBlade" xml:space="preserve">Ostrze Derwisza</s:String>
